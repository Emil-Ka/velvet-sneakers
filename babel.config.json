{
  "presets": [
    "@babel/preset-react",
    "@babel/preset-typescript",
    [
      "@babel/preset-env",
      {
        "useBuiltIns": "usage",
        "corejs": 3
      }
    ]
  ],
  "env": {
    "server": {
      "plugins": [
        [
          "css-modules-transform", {
              "preprocessCss": "./loaders/scss-loader.js",
              "generateScopedName": "[name]__[local]",
              "extensions": [".scss"]
          }
        ]
      ]
    }
}
}